<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NewContent">

    <ListView
        android:id="@+id/listaNewContent1"
        android:layout_width="200dp"
        android:layout_height="523dp"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="DuplicateIds,NotSibling,DuplicateSpeakableTextCheck" />

    <ListView
        android:id="@+id/listaNewContent2"
        android:layout_width="204dp"
        android:layout_height="526dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/listaNewContent1" />

</androidx.constraintlayout.widget.ConstraintLayout>